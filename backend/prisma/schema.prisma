generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  username  String
  email     String
  password  String
  createdAt DateTime @default(now())
}

model Finance {
  id             String   @id @default(uuid())
  nameOfBusiness String
  valueOfFinance String
  recurrence     String
  status         String
  createdAt      DateTime @default(now())
}

model Salary {
  id                  String   @id @default(uuid())
  totalAmountOfSalary String?
  createdAt           DateTime @default(now())
}
